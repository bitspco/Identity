<div id="users" class="modal modal-violet-dark">
    <div>
        <div class="header">
            <div class="title"><i class="mdi mdi-account-multiple-outline"></i> <span class="px-10">لیست اعضاء</span></div>
            <div class="close"><a href="#">✖</a></div>
        </div>
        <div class="body container-xs">
            <table class="table striped bordered">
                <thead>
                    <tr>
                        <th style="width: 70px">شناسه</th>
                        <th>نام</th>
                        <th>نام کاربری</th>
                        <th>وضعیت</th>
                    </tr>
                </thead>
                <tbody>
                    <tr v-for="item in items">
                        <td>{{item.Id}}</td>
                        <td>{{item.Name}}</td>
                        <td>{{item.Username}}</td>
                        <td>{{item.GetStatusName()}}</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
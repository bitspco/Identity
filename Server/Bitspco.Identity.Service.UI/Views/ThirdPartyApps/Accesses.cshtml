<div id="accesses" class="modal modal-violet">
    <div>
        <div class="header">
            <div class="title"><i class="mdi mdi-key"></i> <span class="px-10">لیست دسترسی ها</span></div>
            <div class="close"><a href="#">✖</a></div>
        </div>
        <div class="body container-xs">
            <table class="table striped bordered">
                <thead>
                    <tr>
                        <th style="width: 70px">شناسه</th>
                        <th>نام</th>
                        <th>نام کاربری</th>
                        <th>وضعیت</th>
                    </tr>
                </thead>
                <tbody>
                    <tr v-for="item in items">
                        <td>{{item.ThirdPartyAccess.Id}}</td>
                        <td>{{item.ThirdPartyAccess.Name}}</td>
                        <td>{{item.ThirdPartyAccess.Symbol}}</td>
                        <td>{{item.ThirdPartyAccess.GetStatusName()}}</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>